from turtle import *
screensize(1000, 1000)
tracer(1)
n = 10
down()
right(45)
for i in range(2):
    forward(28 * n)
    right(90)
    forward(12 * n)
    right(90)
forward(7 * n)
left(90)
forward(13 * n)
right(90)
for i in range(2):
    forward(10 * n)
    right(90)
    forward(30 * n)
    right(90)
done()
